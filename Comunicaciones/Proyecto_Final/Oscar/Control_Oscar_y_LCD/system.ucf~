# ==== Clock inputs (CLK) ====
NET "clk" LOC = "B8";
#NET "clk" PERIOD = 20 HIGH 50%;
//NET  led1  LOC = "P4";
NET  rst  LOC = "G18";//SW0



#LEDs tecla pulsada
NET "En_S_Us" LOC = P4; //LD7
NET "LEDs_ON" LOC = E4; //LD6
NET "Melodia_ON" LOC = P16; //LD5
NET "En_Mem" LOC = E16; //LD4
NET "Rst_Mem" LOC = K14; //LD3*
NET "Estado_LCD[2]" LOC = K15; //LD2*
NET "Estado_LCD[1]" LOC = J15; //LD1*
NET "Estado_LCD[0]" LOC = J14; //LD0*

NET "Tecla[0]" LOC = H18; //SW1
NET "Tecla[1]" LOC = K18; //SW2
NET "Tecla[2]" LOC = K17; //SW3
NET "Fin_Cancion" LOC = R17; //SW7
